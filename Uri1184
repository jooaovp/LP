#include <stdio.h>
#include <math.h>

int main(){
    
double M[12][12], a=0;
int l, c, cont=0;
char op;

scanf("%c", &op); //S ou M soma ou media

    for(l=0; l<12; l++)
		for(c=0; c<12; c++){
			scanf("%lf", &M[l][c]);
                if (c<l){
					a=a+M[l][c];
					cont++;
				}
			}

	if (op=='M'){
    	a=a/cont; 
    	printf("%.1lf\n", a);
	}
    if (op=='S'){
    	printf("%.1lf\n", a);
    }

	return 0;
}
